extends ../partials/logbook-includes/logbook-layout

block bodycontent
	div
		h4.text-center #{username}'s Logbook:
			span.small #{totalHours} hours

	.callout.primary.medium-6.float-center.padding-top-1
		if logs.length > 0 || log
			each log in logs
				li= log
		else
			h6.font-bold.text-center No Log Entries!
